@model List<User>
@*
    For more information on enabling MVC for empty projects, visit https://go.microsoft.com/fwlink/?LinkID=397860
*@
@{
}

<h1>Here are all of the users:</h1>

@foreach(User u in Model)
            {
<div>
    <p>@u.FirstName</p>
    <p>@u.FavDrink</p>
    <p>@u.Birthday</p>
    <a class="nav-link text-dark" asp-area="" asp-controller="Home" asp-action="Details" asp-route-id="@u.Id">See more details</a>
</div>
                <br />
            }